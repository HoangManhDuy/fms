<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
	<import resource="spring-dao.xml" />

	<bean id="accountService" parent="baseTransactionProxy">
		<property name="target">
			<bean class="com.project.fms.service.impl.AccountServiceImpl">
				<property name="accountDAO" ref="accountDAO" />
			</bean>
		</property>
	</bean>
	
	<bean id="roomService" parent="baseTransactionProxy">
		<property name="target">
			<bean class="com.project.fms.service.impl.RoomServiceImpl">
				<property name="roomDAO" ref="roomDAO" />
			</bean>
		</property>
	</bean>
	
	<bean id="subjectService" parent="baseTransactionProxy">
		<property name="target">
			<bean class="com.project.fms.service.impl.SubjectServiceImpl">
				<property name="subjectDAO" ref="subjectDAO" />
			</bean>
		</property>
	</bean>
	
</beans>
