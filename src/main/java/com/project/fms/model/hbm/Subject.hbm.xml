<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.project.fms.model">
	<class name="Subject" table="subjects">
		<id name="id" column="id">
			<generator class="increment" />
		</id>

		<property name="name" type="string" column="name" />
		<property name="teacher" type="string" column="teacher" />
		<property name="codeSubject" type="string"
			column="codeSubject" />

		<list name="roomdetails" inverse="true">
			<key column="subjectId" />
			<index column="id" />
			<one-to-many class="RoomDetail" />
		</list>
	</class>
</hibernate-mapping>
