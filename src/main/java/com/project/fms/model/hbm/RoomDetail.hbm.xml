<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.project.fms.model">
	<class name="RoomDetail" table="roomdetails">
		<id name="id" column="id">
			<generator class="increment" />
		</id>

		<property name="date" type="date" column="date" />
		<property name="time_start" type="string" column="time_start" />
		<property name="time_end" type="string" column="time_end" />

		<many-to-one name="room" column="roomId" not-null="true" />
		<many-to-one name="subject" column="subjectId"
			not-null="true" />
	</class>
</hibernate-mapping>
