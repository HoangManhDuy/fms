<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.project.fms.model">
	<class name="Account" table="accounts">
		<id name="id" column="id">
			<generator class="increment" />
		</id>

		<property name="email" type="string" column="email" />
		<property name="password" type="string" column="password" />
		<property name="role" type="integer" column="role" />

		<many-to-one name="profile" column="profile" unique="true"
			not-null="true">
		</many-to-one>

		<list name="rooms" inverse="true" table="roomstudents">
			<key column="accountId" />
			<index column="id" />
			<many-to-many column="roomId" class="Room" />
		</list>

		<list name="requesteds" inverse="true" table="request_accounts">
			<key column="accountId" />
			<index column="id" />
			<many-to-many column="requestId" class="Requested" />
		</list>
	</class>
</hibernate-mapping>
